package Quiz.step1;

import java.util.ArrayList;
import java.util.List;

class A{

}
public class List2 {
    public static void main(String[] args) {
        List<A> list = new ArrayList<>();
        System.out.println(list.size());
        A a = new A();
        list.add(a);
        a = new A();
        list.add(a);
        a = new A();
        list.add(a);
        System.out.println(list.size());
        for(A a1 : list){
            System.out.println(a1);
        }
    }
}
